@model System.Nullable <int>
@if (Model==1)
{
    <text>Plastic sheeting</text>
}
else if (Model == 2)
{
    <text>Blankets/Mats</text>
}
else if (Model==3)
{
    <text>Mosquito nets</text>
}
else if (Model==4)
{
    <text>Kitchen sets</text>
}
else if (Model==5)
{
    <text>Soap</text>
}
else if (Model==6)
{
    <text>Bucket/Jerry can</text>
}
else if (Model==7)
{
    <text>Hygiene Kits</text>
}
else if (Model==8)
{
    <text>None</text>
}
else if (Model==9)
{
    <text>Other</text>
}
else
{
    <text>N/A</text>
}