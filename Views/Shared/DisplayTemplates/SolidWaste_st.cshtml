@model System.Nullable <int>

@if (Model==1)
{
    <text>Very clean</text>
}
else if (Model==2)
{
    <text>Clean</text>
}
else if (Model==3)
{
    <text>Not so clean</text>
}
else if (Model==4)
{
    <text>Dirty</text>
}
else if (Model==5)
{
    <text>Very dirty</text>
}
else
{
    <text>N/A</text>
}